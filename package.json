{
  "name": "deskpro-sdk-react",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "test": "jest ./tests/*/*.test.js",
    "build": "webpack -p",
    "docs:serve": "mkdocs serve",
    "docs:deploy": "gh-pages-deploy",
    "docs:build": "mkdocs build",
    "lint": "eslint src/* --cache --cache-location=.cache/eslint --ext .js,.jsx",
    "lint:fix": "npm run lint -- --fix",
    "storybook": "start-storybook -p 9003 -c .storybook",
    "precommit": "lint-staged",
    "prepublish": "npm run build"
  },
  "author": "DeskPRO <team@deskpro.com>",
  "license": "BSD-3-Clause",
  "devDependencies": {
    "@storybook/react": "^3.2.12",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^2.7.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "documentation": "^5.3.2",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.0.1",
    "eslint-plugin-react": "^7.4.0",
    "gh-pages-deploy": "^0.4.2",
    "jest": "^21.2.1",
    "jsdoc-babel": "^0.3.0",
    "lint-staged": "^4.2.3",
    "module-resolver": "^1.0.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.6.0"
  },
  "dependencies": {
    "@deskproapps/deskproapps-sdk-core": "^1.0.0-beta.18",
    "deskpro-components": "^1.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux-form": "^7.0.4",
    "redux-thunk": "^2.2.0"
  },
  "jest": {
    "modulePaths": [
      "src"
    ]
  },
  "gh-pages-deploy": {
    "staticpath": "site",
    "prep": [
      "docs:build"
    ],
    "noprompt": true
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint"
    ]
  }
}
